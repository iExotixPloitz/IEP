local input
local key = loadstring(game:HttpGet("https://pastebin.com/raw/axuMisFE"))()

function begin()
    rconsoleprint("@@WHITE@@")
    rconsolename("IEP - Key System")
    rconsoleprint("Get Key: https://onepiecered.co/s?P5u \n")
    rconsoleprint("Only 1 checkpoint to Get Key! \n")
    rconsoleprint("\n")
    rconsoleprint("Enter Key: ")
    input = rconsoleinput()


    if input == key then
        rconsoleclear()
        rconsoleprint("@@LIGHT_GREEN@@")
        rconsolename("IEP - Key System")
        rconsoleprint("Welcome!\n")
        wait(1)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/iExotixPloitz/IEP/main/obsHub"))()
        rconsoleprint("Press on |-|\n")
        wait(0.5)
        rconsoleprint("Dont Delete the Console\n")
        wait(0.5)
        rconsoleprint("This console will assure your safety\n")
        wait(0.5)
        rconsoleprint("Go and Have fun now!\n")
        

    elseif input ~= key then
        rconsoleprint("@@RED@@")
        rconsoleprint("Incorrect key!\n")
        rconsolename("Wrong Key!")
        wait(2)
        rconsoleclear()
        begin()
    end
end


begin()
